{
  "expo": {
    "name": "MusikBurk",
    "slug": "MusikBurk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/music-logo.png",
    "splash": {
      "backgroundColor": "#2d283e"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/music-logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.android.mymusic"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
